<%= simple_form_for(@bot) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
  </div>

  <div class="form-inputs">
    <%= f.input :street %>
  </div>

  <div class="form-inputs">
    <%= f.input :city %>
  </div>

  <div class="form-inputs">
    <%= f.input :shop_name %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => 'btn btn-primary' %>
  </div>
<% end %>

<%= content_for(:after_js) do %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;key=<%=ENV['GOOGLE_AUTOCOMPLETE_API']%>"></script>
<% end %>
